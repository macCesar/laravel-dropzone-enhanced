{
  "name": "maccesar/laravel-dropzone-enhanced",
  "description": "A enhanced Dropzone.js component for Laravel with image processing capabilities",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mac Cesar",
      "email": "contact@maccesar.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.2",
    "laravel/framework": "^8.0|^9.0|^10.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6.0|^7.0|^8.0"
  },
  "suggest": {
    "maccesar/laravel-glide-enhanced": "For advanced image processing capabilities"
  },
  "autoload": {
    "psr-4": {
      "MacCesar\\LaravelDropzoneEnhanced\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MacCesar\\LaravelDropzoneEnhanced\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MacCesar\\LaravelDropzoneEnhanced\\DropzoneServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
